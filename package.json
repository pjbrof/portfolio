{
  "version": "0.1.0",
  "description": "Patrick Brophy Portfolio",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "start": "node index.js",
    "dev": "webpack serve",
    "test": "jest --watchAll",
    "resume": "http-server ./resume -c10"
  },
  "repository": "",
  "keywords": [],
  "author": "Patrick Brophy",
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.2.0",
    "enzyme": "^3.11.0",
    "eslint": "^8.16.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "http-server": "^14.1.1",
    "jest": "^28.1.0",
    "postcss": "^8.4.14",
    "style-loader": "^3.2.1",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.9.1"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "express": "^4.17.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "document": false
    },
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "arrow-body-style": 1
    }
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./setupTest.js",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|scss|sass|less)$": "identity-obj-proxy"
    }
  }
}
